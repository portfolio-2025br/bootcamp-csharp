{
  "version": "2.0.0",
  "_comment": "Configura as tarefas de compilação e limpeza do projeto.",
  "tasks": [
    {
      "label": "::build",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/WEX - End to End.sln"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "::clean",
      "command": "dotnet",
      "type": "process",
      "args": ["clean", "${workspaceFolder}/WEX - End to End.sln"],
      "problemMatcher": "$msCompile"
    }
  ]
}
